{
  "name": "node-starter",
  "version": "1.0.0",
  "description": "Starter kit for Node.js projects",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --config ./node_modules/@fpoumian/common/eslint-configs/node-standard/.eslintrc",
    "format": "prettier ./**/*.js --config ./node_modules/@fpoumian/common/.prettierrc --write",
    "test": "jest"
  },
  "author": "Fernando Poumian",
  "license": "MIT",
  "devDependencies": {
    "@fpoumian/common": "^1.0.1",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/?(*.)(spec|test).js"
    ],
    "testEnvironment": "node"
  }
}
